    public ListNode reverseList(ListNode head) {
     
    	return null;
    }
	public static void main(String[] args) {
		
		ListNode node = new ListNode();
		
		node.addArray(new int[] {1,2,3,4,5});
		
		int t = 0;
		t++;
	}

}
